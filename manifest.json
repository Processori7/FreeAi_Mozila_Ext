{
  "icons": {
    "32": "icon-32.png",
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Free Ai Services",
  "version": "16.6.35",
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage",
    "sidebar"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open Free AI Services Sidebar"
  },
  "sidebar_action": {
    "default_title": "Free Ai Services",
    "default_panel": "popup.html",
    "default_icon": "icon.png",
    "open_at_install": true,
    "browser_style": false
  },
  "background": {
    "scripts": ["background.js"]
  }
}